<h1>Top 5 Population</h1>

<table class="table">
	<tr>
		<th>Name</th>
		<th>capital</th>
		<th>population</th>
		<th>area</th>
	</tr>
	<% @states.each do |state| %>
		<tr>
			<td><%= state.name %></td>
			<td>
				<%= state.capital %>
			</td>
			<td>
				<%= state.population %>
			</td>
			<td>
				<%= state.area %>
			</td>	
		</tr>
	<% end %>
</table>

<%= link_to "Back Home", root_path %>